{
  "name": "unifiedfieldtheory.cn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "format": "pnpm run format:code",
    "format:ci": "pnpm run format:imports && pnpm run format:code",
    "format:code": "prettier -w . --cache --plugin-search-dir=.",
    "format:imports": "organize-imports-cli ./tsconfig.json",
    "add-language": "node ./scripts/add-language.mjs",
    "docgen": "node ./scripts/docgen.mjs",
    "docgen:errors": "node ./scripts/error-docgen.mjs",
    "lint:a11y": "start-test 'yarn build && yarn preview' 3000 'yarn lint:a11y:local'",
    "lint:a11y:local": "pa11y-ci --sitemap 'http://localhost:3000/sitemap.xml' --sitemap-find 'https://docs.astro.build' --sitemap-replace 'http://localhost:3000'",
    "lint:a11y:remote": "pa11y-ci --sitemap 'https://unifiedfieldtheory.cn/sitemap.xml'",
    "lint:linkcheck": "SKIP_OG=true pnpm build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "node ./scripts/lint-slugcheck.mjs",
    "lint:eslint": "eslint .",
    "netlify:build": "pnpm ${NETLIFY_BUILD_SCRIPT:-build}",
    "lunaria:build": "lunaria build",
    "lunaria:preview": "lunaria preview",
    "short:url": "node ./scripts/short-url.mjs"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^4.0.1",
    "@actions/core": "^1.11.1",
    "@babel/core": "^7.26.0",
    "@docsearch/css": "^3.7.0",
    "@types/canvas-confetti": "^1.6.4",
    "@types/hast": "^3.0.4",
    "@types/html-escaper": "^3.0.2",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "astro": "4.16.10",
    "astro-auto-import": "^0.4.4",
    "astro-eslint-parser": "^1.0.3",
    "astro-og-canvas": "^0.5.4",
    "bcp-47-normalize": "^2.3.0",
    "canvaskit-wasm": "^0.39.1",
    "dedent-js": "^1.0.1",
    "domhandler": "^5.0.3",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react": "^7.37.2",
    "fast-glob": "^3.3.2",
    "hast-util-from-html": "^2.0.3",
    "hast-util-select": "^6.0.3",
    "hast-util-to-html": "^9.0.3",
    "hast-util-to-string": "^3.0.1",
    "hastscript": "^9.0.0",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^9.1.0",
    "kleur": "^4.1.5",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-hast": "^13.2.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark-util-character": "^2.1.0",
    "micromark-util-symbol": "^2.0.0",
    "node-fetch": "^3.3.2",
    "organize-imports-cli": "^0.10.0",
    "p-retry": "^6.2.0",
    "parse-numeric-range": "^1.3.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prompts": "^2.4.2",
    "rehype": "^13.0.2",
    "remark": "^15.0.1",
    "remark-directive": "^3.0.0",
    "tsm": "^2.3.0",
    "typescript": "^5.6.3",
    "unified": "^11.0.5",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-walker": "^1.0.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/starlight": "^0.29.0",
    "@astrojs/vercel": "^7.8.2",
    "@docsearch/js": "^3.7.0",
    "@expressive-code/plugin-collapsible-sections": "^0.38.3",
    "@fontsource/ibm-plex-mono": "^5.1.0",
    "@lunariajs/core": "^0.1.1",
    "canvas-confetti": "^1.9.3",
    "jsdoc-api": "^9.3.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-smartypants": "^2.0.0",
    "sass": "^1.80.6",
    "sharp": "^0.33.5"
  },
  "packageManager": "pnpm@9.3.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "vite"
      ]
    }
  }
}
